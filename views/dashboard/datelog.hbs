<img src="../images/Astrolover-logo.png" alt="logo" class="logo">

  
  <h2>Hello {{username}}</h2>
  <img src ={{image_url}}>
  <ul class="nav nav-tabs">
        <li class="nav-item">
          <a class="nav-link " href="/dashboard/home">Home</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/dashboard/myPotentials">My Potentials</a>
        </li>
        <li class="nav-item">
          <a class="nav-link active" href="/dashboard/datelog">Datelog</a>
        </li>
      </ul>

  <section class="datelog-box">
  
  
    <div class="card-request">
      <p>I SENT REQUEST TO...</p>
      {{#each senderarr}}
      <div class="eachlover">
        <img   src={{receiverId.image_url}}>
        <div class="lover-text">
        <p>{{receiverId.username}}</p>
        <p>Status: {{status}} </p>
        </div>
        {{!-- <button><a href="/profile/{{this.receiverId._id}}">Go to profile</a></button> --}}
      </div>
      {{/each}}
    </div>
  
    <div class="card-request">
      <p>I RECEIVED REQUEST FROM </p>
      {{#each receiverarr}}
      <div>{{senderId.username}} {{status}}</div>
      {{/each}}
    </div>
  
    <div class="card-request">
      <p>list of people that status is confirmed</p>
    </div>
  
  </section>



<button><a href="/logout">LOGOUT</a></button>