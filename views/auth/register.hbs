<img src="./images/Astrolover-logo.png" alt="logo" class="logo">
<div class="login-box">
<h3>Welcome!</h3>
<form class="register-form"action="/register" method="POST">
    <label >Email</label>
    <input type="email" name="email" placeholder="Enter email">
    <label >Username</label>
    <input type="name" name="username" placeholder="Enter username">
    <label >Password</label>
    <input type="password" name="password" placeholder="Enter password">
    <label >Date of birth</label>
    <input type="text" name="dateOfBirth" placeholder="dd/mm/yyyy">
    <button  class="login-btn"type="submit">Register</button>
</form>
</div>



{{#if message}}
<p>{{message}}</p>
{{/if}}

<button><a href="/login">Back to LogIn</a></button>
<button><a href="/logout">LOGOUT</a></button>
